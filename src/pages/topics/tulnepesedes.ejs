              <div class="media-body">
                <h4 class="h3 mt0">
                  <a href="<%= postsDataForIndexPage[i].pathToPost %>">
                    <%= postsDataForIndexPage[i].title %>
                  </a>
                </h4>
                <p class="medium-size"><%= postsDataForIndexPage[i].excerpt %></p>
              </div>
            </div>
          </li>
          <% } %>
          <% } %>
