                <ul class="no-bullets m0">
                <% for (var i = 0, j = 0; i < postsDataForIndexPage.length; i++) { %>
                  <% if (postsDataForIndexPage[i].topic === 'erőforrások') { %>
                  <li class="pbhalf fs-22">
                    <a href="<%= postsDataForIndexPage[i].pathToPost %>" class="black-link">
                      <%= postsDataForIndexPage[i].title %>
                    </a> 
                  </li>
                  <% } %>
                <% } %>
                </ul>
              </div>
            </div>
